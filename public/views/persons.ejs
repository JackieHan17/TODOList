<h2>List</h2>
<hr></<hr>
<% if (userinfos.length) { %>
  <div class="container">
    <div class="row centered">
    <% userinfos.forEach(function(userinfo){ %>
    <div class="col-lg-1 bd">
        <div class='name'>
          <h3><%= userinfo['title'] %></h3>
        </div>
        <a class="btn btn-info read" href="#" rel=<%= userinfo['_id']%>><i class="fa fa-book fa-lg"></i> View</a></br>
        <a class="btn btn-danger delete mt" href="#" rel=<%= userinfo['_id']%>><i class="fa fa-trash-o fa-lg"></i> Delete</a></br>
        <a class="btn btn-success update mt" href="#" rel=<%= userinfo['_id']%>><i class="fa fa-pencil fa-lg"></i> Edit</a></br>
        <!-- <a href="" class='read' rel=<%= userinfo['_id']%>>VIEW</a></br> -->
        <!-- <a href="" class='delete' rel=<%= userinfo['_id']%>>DEL</a>></br> -->
        <!-- <a href="" class='update' rel=<%= userinfo['_id']%>>EDIT</a></br> -->
    </div>
    <% }) %>
    </div>
  </div>
<% } %>
